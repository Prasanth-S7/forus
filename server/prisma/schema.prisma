generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Inventory{
  id   Int   @id @default(autoincrement())
  inventoryName String
  inventoryAddress String
  authorityIncharge String
  inventoryManager String
  Manageraddress String
  contact String
  mailId String
}

model Good{
  id Int @id @default(autoincrement())
  name String
  type String
  Quantity Int
}